# Project configuration for workflow-dispatch agent
# Copy this file to projects.yaml and customize for your projects
# NOTE: projects.yaml is gitignored and should NOT be committed

projects:
  # Test repository - use this to test the tool
  test:
    repo: RahatHameed/runlocal-test
    workflow: test.yaml
    branch: main
    defaults:
      message: "Hello from runlocal!"
      sleep_seconds: "15"

  # Choice test - demonstrates parameter validation with auto-correction
  choice-test:
    repo: RahatHameed/runlocal-test
    workflow: choice-test.yaml
    branch: main
    defaults:
      environment: "Development"
      database: "MySQL8.0"

  # Add more projects as needed:
